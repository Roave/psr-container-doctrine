{
    "name": "roave/psr-container-doctrine",
    "description": "Doctrine Factories for PSR-11 Containers",
    "license": "BSD-2-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Ben Scholzen",
            "homepage": "https://github.com/dasprid"
        },
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com",
            "homepage": "https://github.com/asgrim"
        }
    ],
    "homepage": "https://github.com/Roave/psr-container-doctrine",
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "doctrine/common": "^3.4.4",
        "doctrine/dbal": "^4.0.2",
        "doctrine/event-manager": "^2",
        "doctrine/migrations": "^3.7.4",
        "doctrine/orm": "^3.2.0",
        "doctrine/persistence": "^3.3.2",
        "psr/cache": "^2.0.0 || ^3.0.0",
        "psr/container": "^1.1.2 || ^2.0.2"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0.0",
        "phpunit/phpunit": "^10.5.20",
        "psalm/plugin-phpunit": "^0.19.0",
        "symfony/yaml": "^7.0.7",
        "vimeo/psalm": "^5.24.0"
    },
    "autoload": {
        "psr-4": {
            "Roave\\PsrContainerDoctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoaveTest\\PsrContainerDoctrine\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ocramius/package-versions": true
        },
        "platform": {
            "php": "8.2.99"
        },
        "sort-packages": true
    }
}
